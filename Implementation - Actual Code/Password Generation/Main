package passwordgenerator;

import java.awt.EventQueue;

public class Main {   //runningthe program

    private static void loadGUI() {  
            EventQueue.invokeLater(() -> {
                GUI gui = new GUI();
                gui.createAndShowGUI();  //all gui is intitalized
                gui.setVisible(true);
            });
    }

    public static void main(String[] args) {
        loadGUI();   //loading the gui
    }
}
